<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProjeDenemesi1</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
@*    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProjeDenemesi1.styles.css" asp-append-version="true" />
</head>
<body>
    <nav class="navbar navbar-inverse" style="background-color:darkslategray">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">DOY</a>
            </div>
            <ul class="nav navbar-nav">
                <li><a asp-controller="Category" asp-action="Index">Kategoriler</a></li>
                <li><a asp-controller="Food" asp-action="Index">Yiyecekler</a></li>
                <li><a asp-controller="Chart" asp-action="Index">Google Chart</a></li>
                <li><a asp-controller="" asp-action="">Popup</a></li>
                <li><a asp-controller="Chart" asp-action="Statistics">İstatistik</a></li>
                <li><a asp-controller="" asp-action="">Ayarlar</a></li>
                <li><a asp-controller="" asp-action="">Çıkış</a></li>
            </ul>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
        @*<main role="main" class="pb-3">
            @RenderBody()
        </main>*@
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - ProjeDenemesi1 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
